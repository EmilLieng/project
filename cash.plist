<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.apple.cash</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/python3</string>
        <string>/tmp/.cash/cash.py</string>
    </array>
    <key>StartCalendarInterval</key>
    <dict>
        <key>Hour</key>
        <integer>2</integer> <!-- Replace with the hour you want -->
        <key>Minute</key>
        <integer>0</integer> <!-- Replace with the minute you want -->
    </dict>
    <key>RunAtLoad</key>
    <true/>
</dict>
</plist>
